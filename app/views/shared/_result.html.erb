<%# <% link_to game_result_path(result.game, result) do %1> %>
<%# <% end %1> %>

<% if result.teams.count > 2 && result.winners.count == 1 %>
  <%= render :partial => 'shared/competition_result', locals: { result: result, options: options } %>
<% elsif result.players.count == 2 %>
  <%= render :partial => 'shared/singles_result', locals: { result: result, options: options } %>
<% elsif result.players.count > 4 || result.teams.map { |t| t.players.count  }.any? { |s| s > 2  } %>
  <%= render :partial => 'shared/large_team_result', locals: { result: result, options: options } %>
<% elsif result.players.count == 4 %>
  <%= render :partial => 'shared/doubles_result', locals: { result: result, options: options } %>
<% end %>
